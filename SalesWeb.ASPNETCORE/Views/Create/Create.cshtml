@model SalesWeb.ASPNETCORE.Models.Entities.Sellers

@{
    ViewData["Title"] = "Create";
}

<h1>@ViewData["Title"]</h1>

<h4>Seller</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div class="form-group">
                <label asp-for="name" class="custom-control-label"></label>
                <input asp-for="name" class="custom-control-label"></>
            </div>
            <div class="form-group">
                <label asp-for="email" class="custom-control-label"></label>
                <input asp-for="email" class="custom-control-label"></>
            </div>
            <div class="form-group">
                <label asp-for="bithDate" class="custom-control-label"></label>
                <input asp-for="bithDate" class="custom-control-label"></>
            </div>
            <div class="form-group">
                <label asp-for="baseSalary" class="custom-control-label"></label>
                <input asp-for="baseSalary" class="custom-control-label"></>
            </div>
            <div class="form-group">
                <input type="submit" class="btn btn-dark"/>
            </div>

        </form>
    </div>
</div>
